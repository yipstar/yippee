<launch>
  <node pkg="mavros" type="mavros_node" name="mavros_node">
    <param name="fcu_url" value="/dev/ttyACM0:115200" />
    <param name="gcs_url" value="udp://@192.168.77.15" />
  </node>

  <node pkg="yippee" type="record_node.py" name="record_node" />

  <include file="$(find jetson_csi_cam)/jetson_csi_cam.launch">
    <arg name="width" value="640" />
    <arg name="height" value="480" />
  </include>

</launch>
